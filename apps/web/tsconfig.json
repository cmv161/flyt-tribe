{
  "extends": "@flyt-tribe/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@flyt-tribe/ui/*": ["../../packages/ui/src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "declaration": false,
    "declarationMap": false,
    "emitDeclarationOnly": false
  },
  "include": [
    "next-env.d.ts",
    "next.config.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "../../packages/ui/src/components/compounds/nutrients-promo.tsx"
  ],
  "exclude": ["node_modules"]
}
